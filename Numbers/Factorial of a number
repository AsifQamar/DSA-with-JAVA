import java.util.Scanner;

public class fact {


    public static long facto(long n) {
        if (n == 0 || n == 1)
            return 1;
        else
            return n * facto(n - 1);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int n = sc.nextInt();
        long result = facto(n);

        System.out.println("Factorial of " + n + " is: " + result);
    }
}
